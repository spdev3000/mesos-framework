    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "~1.1.3",
    "gulp": "~3.9.1",
    "gulp-istanbul": "~1.1.2",
    "gulp-mocha": "~4.3.1",
    "istanbul": "0.4.5",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
